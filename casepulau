local player = game.Players.LocalPlayer
local pGui = player:WaitForChild("PlayerGui")
local target = pGui:FindFirstChild("OpenAnimation")

if target then
    -- Kita buat folder "Tempat Sampah" yang tidak akan pernah dirender oleh game
    local bin = Instance.new("Folder")
    bin.Name = "HiddenTrash"
    bin.Parent = game:GetService("LogService") -- Tempat yang jarang dicek anti-cheat
    
    -- Pindahkan foldernya ke sana
    target.Parent = bin
    print("Folder OpenAnimation sudah dibuang ke HiddenTrash!")
else
    warn("Folder OpenAnimation tidak ditemukan, mungkin sudah pindah?")
end

-- Mencegah game membuat folder OpenAnimation baru
pGui.ChildAdded:Connect(function(child)
    if child.Name == "OpenAnimation" then
        task.wait()
        child.Parent = game:GetService("LogService")
        print("Folder baru terdeteksi dan langsung dibuang!")
    end
end)
